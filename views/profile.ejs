<!DOCTYPE html>
<html>
<head>
	<title>SignUrDoc</title>
	<link rel="stylesheet" type="text/css" href="/css/profil.css">
  <link rel="stylesheet" type="text/css" href="/css/style.css">
  <link rel="stylesheet" type="text/css" href="/css/login.css">
  <link rel="stylesheet" type="text/css" href="/css/upload.css">
  <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css">
</head>
<body>
  <nav>
    <div class="logo-name">

        <span class="logo_name">SignUrDoc</span>
    </div>

    <div class="menu-items">
      <ul class="nav-links">
        <li><a href="/home">
            <i class="uil uil-estate"></i>
            <span class="link-name">Dashboard</span>
        </a></li>
        <li><a href="/signature/signature">
            <i class="uil uil-pen"></i>
            <span class="link-name">Upload Signature</span>
        </a></li>
        <li><a href="/document/document">
            <i class="uil uil-files-landscapes"></i>
            <span class="link-name">Upload Document</span>
        </a></li>
        <li><a href="/profile/profile">
            <i class="uil uil-user-circle"></i>
            <span class="link-name">Profile</span>
        </a></li>
    </ul>
        
        <ul class="logout-mode">
            <li><a href="login.html">
                <i class="uil uil-signout"></i>
                <span class="link-name">Logout</span>
            </a></li>
        </li>
        </ul>
    </div>
</nav>

<section class="dashboard">
  <div class="dash-content">

      <div class="activity">
          <div class="title">
              <i class="uil uil-user-circle"></i>
              <span class="text">Profile</span>
          </div>
	     </div>
       <div class="profile">
        <div class="card mb-4 ">
          <div class="card-header">Profile Picture</div>
          <div class="card-body text-center profil-card">
              <!-- Profile picture image-->
              <img class="img-account-profile rounded-circle mb-5" src="/images/profile.jpg" alt="">
              <break></break>
              <div class="upload-file">
                <input type="file" id="file-upload" class="file-input">
                <label for="file-upload" class="upload-button">Change Picture</label>
                <span class="file-name">No File Choosen</span>
              </div>
          </div>
  </div>
</div>
<form>
    <div class="form-wrapper">
        <div class="form-right">
          <div class="form-group">
            <label for="email">Email:</label>
            <input type="text" id="email" name="email" value="">
          </div>
          <div class="form-group">
            <label for="username">Username:</label>
                <input type="text" id="username" name="username" value="">
          </div>
          <div class="form-group">
            <label for="password">Password:</label>
                <input type="password" id="password" name="password" value="">
          </div>
            
          </div>
        </div>
      </div>
      <form action="submit" method="get">
        <div class="form-group">
            <input type="submit" value="Update">
          </div>
        </form>
      
      
      
</body>